<data>


    <xpath expr="/tree/field[@name='max_quantity']" position="replace">
         <field name="max_quantity" tree_invisible="1"/>
    </xpath>

    <xpath expr="/tree/field[@name='min_quantity']" position="replace">
         <field name="min_quantity" tree_invisible="1"/>
    </xpath>
    <xpath expr="/tree/field[@name='product']" position="before">
        <field name="code"/>
        <field name="name"/>
    </xpath>

    <xpath expr="/tree/field[@name='product']" position="replace">
        <field name="product" tree_invisible="1"/>
    </xpath>
</data>
